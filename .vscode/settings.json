{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/*.d.ts": true,
        "**/*.d.ts.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    }
}